<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<TangentWave fileType="ControlSystem" fileVersion="3.0">
    <Capabilities>
        <Jog enabled="true"/>
        <Shuttle enabled="false"/>
        <StatusDisplay lineCount="3"/>
        <CustomControls enabled="true"/>
    </Capabilities>
    <Modes>
        <Mode id="0x00000001">
            <Name>Develop</Name>
        </Mode>
    </Modes>
    <Controls>
	<Group name="Navigation">
            <Action id="0x00010001">
                <Name>Previous</Name>
            </Action>
             <Action id="0x00010002">
                <Name>Next</Name>
            </Action>
	</Group>

        <Group name="Adjust">
            <Action id="0x00021000">
                <Name>Undo</Name>
                <Name9>Undo</Name9>
            </Action>
            <Action id="0x00021001">
                <Name>Redo</Name>
                <Name9>Redo</Name9>
            </Action>
            <Action id="0x00021002">
                <Name>Auto Tone</Name>
                <Name9>Auto Tone</Name9>
            </Action>

            <Parameter id="0x00020001">
                <Name>Color Temp</Name>
                <Name9>ColorTemp</Name9>
                <Name20>Color Temp</Name20>
                <MinValue>0</MinValue>
                <MaxValue>1</MaxValue>
                <StepSize>0.0001</StepSize>
            </Parameter>
            <Parameter id="0x00020002">
                <Name>Tint</Name>
                <Name20>Tint</Name20>
                <MinValue>0</MinValue>
                <MaxValue>1</MaxValue>
                <StepSize>0.0001</StepSize>
            </Parameter>

            <Parameter id="0x00020003">
                <Name>Exposure</Name>
                <Name9>Exposure</Name9>
                <Name20>Exposure</Name20>
                <MinValue>0</MinValue>
                <MaxValue>1</MaxValue>
                <StepSize>0.0001</StepSize>
            </Parameter>
            <Parameter id="0x00020004">
                <Name>Highlights</Name> <!-- Recovery in V1/V2 -->
                <Name9>Highlight</Name9>
                <Name20>Highlights</Name20> <!-- Recovery in V1/V2 -->
                <MinValue>0</MinValue>
                <MaxValue>1</MaxValue>
                <StepSize>0.0001</StepSize>
            </Parameter>
            <Parameter id="0x00020005"> <!-- Fill Light in V1/V2 -->
                <Name>Shadows</Name>
                <Name20>Shadows</Name20>
                <MinValue>0</MinValue>
                <MaxValue>1</MaxValue>
                <StepSize>0.0001</StepSize>
            </Parameter>
            <Parameter id="0x00020006">
                <Name>Brightness</Name> <!-- V1/V2 only -->
                <Name9>Brightnes</Name9>
                <Name20>Brightness</Name20> <!-- V1/V2 only -->
                <MinValue>0</MinValue>
                <MaxValue>1</MaxValue>
                <StepSize>0.0001</StepSize>
            </Parameter>
            <Parameter id="0x00020007">
                <Name>Contrast</Name>
                <Name20>Contrast</Name20>
                <MinValue>0</MinValue>
                <MaxValue>1</MaxValue>
                <StepSize>0.0001</StepSize>
            </Parameter>
            <Parameter id="0x00020008">
                <Name>Blacks</Name>
                <Name20>Blacks</Name20>
                <MinValue>0</MinValue>
                <MaxValue>1</MaxValue>
                <StepSize>0.0001</StepSize>
            </Parameter>
            <Parameter id="0x00020009">
                <Name>Whites</Name>
                <Name20>Whites</Name20>
                <MinValue>0</MinValue>
                <MaxValue>1</MaxValue>
                <StepSize>0.0001</StepSize>
            </Parameter>
            <Parameter id="0x0002000a">
                <Name>Clarity</Name>
                <Name20>Clarity</Name20>
                <MinValue>0</MinValue>
                <MaxValue>1</MaxValue>
                <StepSize>0.0001</StepSize>
            </Parameter>
            <Parameter id="0x0002000b">
                <Name>Vibrance</Name>
                <Name20>Vibrance</Name20>
                <MinValue>0</MinValue>
                <MaxValue>1</MaxValue>
                <StepSize>0.0001</StepSize>
            </Parameter>
            <Parameter id="0x0002000c">
                <Name>Saturation</Name>
                <Name20>Saturation</Name20>
                <MinValue>0</MinValue>
                <MaxValue>1</MaxValue>
                <StepSize>0.0001</StepSize>
            </Parameter>
        </Group>
    </Controls>
    <DefaultGlobalSettings>
        <KnobSensitivity std="1" alt="5"/>
        <JogDialSensitivity std="1" alt="5"/>
        <TrackerballSensitivity std="1" alt="5"/>
        <TrackerballDialSensitivity std="1" alt="5"/>
        <IndependentPanelBanks enabled="false"/>
    </DefaultGlobalSettings>
</TangentWave>
